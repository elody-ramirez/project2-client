{{#each players as |player|}}
  {{log player}}
  <section data-id="{{player.id}}">
    <h6>Name:</h6>
    <h3>
      {{limit player.name 15}}
    </h2>
    <h6>Rating:</h6>
    <p>
      {{#if player.rating}}
        {{player.rating}}
      {{else}}
        No rating provided.
      {{/if}}
    </p>
    {{> partial player}}
  </section>

{{/each}}
